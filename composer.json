{
    "name": "google/gax",
    "type": "library",
    "description": "Google API Core for PHP",
    "keywords": ["google"],
    "homepage": "https://github.com/googleapis/gax-php",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "google/auth": "dev-master as 1.2.0",
        "grpc/grpc": "^1.4",
        "google/protobuf": "3.4.*",
        "guzzlehttp/promises": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Google\\": "src",
            "GPBMetadata\\Google\\": "metadata"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Google\\ApiCore\\UnitTests\\": "tests"
        }
    }
}
