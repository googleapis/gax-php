{
    "name": "google/gax",
    "type": "library",
    "description": "Google API Extensions for PHP",
    "keywords": ["google"],
    "homepage": "https://github.com/googleapis/gax-php",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "google/auth": "0.7",
        "grpc/grpc": "v0.15.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
          "Google\\GAX\\":"src/"
        },
        "classmap": ["src/generated"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
